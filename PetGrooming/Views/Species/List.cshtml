@model List<PetGrooming.Models.Species>
@{
    ViewBag.Title = "List";
}

<h2>List of Species</h2>
<a href="/Species/Add">Add New Species</a>
@{ 
    foreach (var Species in @Model) {

    <div class="Species_list">
        <span><a href="/Species/Delete/@Species.SpeciesID" class="deleteSpecies_link">Delete</a></span>
        <span><a href="/Species/Show/@Species.SpeciesID">@Species.Name</a></span>
    </div>
    }
}

